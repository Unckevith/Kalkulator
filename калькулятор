class Kalkulator:
    def addition(a,b):
        return a+b
    def subtraction(a,b):
        return a-b
    def division(a,b):
        return a/b
    def multiplication(a,b):
        return a*b
    def degree(a,b):
        return a**b
a=int(input("Первое"))
b=int(input("Второе"))
alkash = Kalkulator()  
print(alkash.addition()) 
print(alkash.subtraction())
print(alkash.division())
print(alkash.multiplication())
print(alkash.degree())
