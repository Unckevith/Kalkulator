class Kalkulator:
    def addition(a,b):
        return a+b
    def subtraction(a,b):
        return a-b
    def division(a,b):
        return a/b
    def multiplication(a,b):
        return a*b
    def degree(a,b):
        return a**b
Kalkulator().addition()
Kalkulator().subtraction()
Kalkulator().division()
Kalkulator().multiplication()
Kalkulator().degree()
