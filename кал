class Kalkulator:
    def addition(a,b):
        return a+b
    def subtraction(a,b):
        return a-b
    def division(a,b):
        return a/b
    def multiplication(a,b):
        return a*b
    def degree(a,b):
        return a**b
alkash=Kalkulator
alkash.addition(1,2)
alkash.subtraction(1,2)
alkash.division(1,2)
alkash.multiplication(1,2)
alkash.degree(1,2)
